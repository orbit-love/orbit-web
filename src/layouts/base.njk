<!DOCTYPE html>
<html lang="{{ meta.lang }}" class="bg-white antialiased">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="google-site-verification" content="puPxunTT1ZsUcpiocDLnr5Ofz7yGjlfJy4P584bezmU" />
    <title>{{ renderData.title or title or meta.title }}</title>
    {% if build.env === 'production' %}
      <script>
        !function(){var analytics=window.analytics=window.analytics||[];if(!analytics.initialize)if(analytics.invoked)window.console&&console.error&&console.error("Segment snippet included twice.");else{analytics.invoked=!0;analytics.methods=["trackSubmit","trackClick","trackLink","trackForm","pageview","identify","reset","group","track","ready","alias","debug","page","once","off","on","addSourceMiddleware","addIntegrationMiddleware","setAnonymousId","addDestinationMiddleware"];analytics.factory=function(e){return function(){var t=Array.prototype.slice.call(arguments);t.unshift(e);analytics.push(t);return analytics}};for(var e=0;e<analytics.methods.length;e++){var key=analytics.methods[e];analytics[key]=analytics.factory(key)}analytics.load=function(key,e){var t=document.createElement("script");t.type="text/javascript";t.async=!0;t.src="https://cdn.segment.com/analytics.js/v1/" + key + "/analytics.min.js";var n=document.getElementsByTagName("script")[0];n.parentNode.insertBefore(t,n);analytics._loadOptions=e};analytics._writeKey="O3JyHBNHK8ZEi8frCGHtmktD7MfEYOca";analytics.SNIPPET_VERSION="4.13.2";
        analytics.load("O3JyHBNHK8ZEi8frCGHtmktD7MfEYOca");
        analytics.page();
        }}();
      </script>
      <script src="https://dog.orbit.love/script.js" data-site="XEYVCUXY" defer></script>
    {% endif %}
    <link rel="stylesheet" href="{{ '/assets/main.css' | url }}" />
    <link rel="stylesheet" href="{{ '/prism.css' | url }}" />
    {% include "meta.njk" %}
  </head>
  <body class="text-gray-900 leading-normal">
    <div hidden>{% svgsprite %}</div>
    <a href="#content" class="sr-only">skip to main content</a>
    <div
      role="document"
      class="min-h-screen flex flex-col"
      x-data="{ showModal: false }"
      @keydown.escape="showModal = false"
      @togglemodal="showModal = $event.detail.value"
    >
      <main class="flex-grow w-full">
        {{ content | safe }}
      </main>
      {% include "contact-form.njk" %}
    </div>
    <script src="{{ '/assets/main.js' | url }}"></script>
    <!-- LAST BUILD: {{ build.env}} {{ build.timestamp | dateToISO }} -->
  </body>
  {% include "alpine-methods.njk" %}
</html>
